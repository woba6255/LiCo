{
  "compilerOptions": {
    "module": "es2022",
    "target": "esnext",
    "moduleResolution": "Bundler",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "types": [
      "vite/client"
    ]
  }
}
